{
  "version": 2,
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python",
       "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "environment": {
        "SECRET_KEY": "${SECRET_KEY}",
        "DEBUG": "${DEBUG}"
       }
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/static/images/ico/favicon.ico"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ]
}